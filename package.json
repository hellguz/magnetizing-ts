{
  "name": "magnetizing-fpg-ts",
  "version": "0.1.0",
  "description": "High-performance headless TypeScript library for procedural floor plan generation",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "dev": "tsc --watch",
    "example": "yarn build && tsx examples/basic-usage.ts",
    "example:corridors": "yarn build && tsx examples/corridors-example.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "floor-plan",
    "procedural-generation",
    "evolutionary-algorithm",
    "physics-simulation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/html": "^7.6.0",
    "@storybook/html-vite": "^7.6.0",
    "@vitest/ui": "^4.0.14",
    "leva": "^0.9.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "clipper-lib": "^1.0.0"
  }
}
